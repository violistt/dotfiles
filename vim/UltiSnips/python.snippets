from collections import deque

def bfs(start):
    visited = [0] * len(g)
    visited[start] = 1
    queue = deque()
    queue.append(start)
    dist = [-1] * len(g)
    dist[start] = 0
    while (queue):
        n = queue.popleft()
        for sus in g[n]:
            if (not visited[sus]):
                queue.append(sus)
                dist[sus] = dist[n] + 1
                visited[sus] = 1
    return dist
