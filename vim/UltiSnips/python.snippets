import heapq
def dijkstra(start):
    dist = [inf] * len(g)
    dist[start] = 0
    root = [-1] * len(g)
    queue = []
    heapq.heappush(queue, (dist[start], start))
    while (queue):
        w, n = heapq.heappop(queue)
        if (dist[n] < w):
            continue
        for sus_n, sus_w in g[n]:
            if (w + sus_w < dist[sus_n]):
                dist[sus_n] = w + sus_w
                root[sus_n] = n
                heapq.heappush(queue, (dist[sus_n], sus_n))
    return dist, root
endsnippet

